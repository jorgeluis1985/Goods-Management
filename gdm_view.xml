<openerp>
    <data>
        <record model="ir.ui.view" id="supplier_tree_view">
                <field name="name">gdm.supplier.tree</field>
                <field name="model">gdm.supplier</field>
                <field name="arch" type="xml"/>
                <tree String="Supplier">
                    <field name="name"/>
                    <field name="city"/>
                    <field name="state"/>
                    <field name="country"/>
                    <field name="zip"/>
                    <field name="email"/>
                    <field name="phone"/>
                    <field name="mobile"/>
                    <field name="website"/>
                </tree>
            </record>
    </data>
</openerp>